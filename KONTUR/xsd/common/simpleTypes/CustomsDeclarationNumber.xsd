<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:annotation>
    <xsd:documentation>Номер таможенной декларации</xsd:documentation>
  </xsd:annotation>
  <xsd:simpleType name="CustomsDeclarationNumberType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\d{8}/\w{6}/\w{7}/\d{1,3}"/>
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>
