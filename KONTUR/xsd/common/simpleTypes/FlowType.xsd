<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:simpleType name = "FlowType">
    <xsd:annotation>
      <xsd:documentation>Тип поставки, может принимать значения: Stock - сток до РЦ, Transit - транзит в магазин</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base ="xsd:string">
      <xsd:enumeration value ="Stock"/>
      <xsd:enumeration value ="Transit"/>
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>

