<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:annotation>
    <xsd:documentation>2-буквенный код страны, согласно ISO-3166</xsd:documentation>
  </xsd:annotation>
  <xsd:simpleType name="CountryISOCodeType">
    <xsd:restriction base="xsd:string">
      <xsd:length value="2"/>
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>
